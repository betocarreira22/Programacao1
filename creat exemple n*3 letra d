#include <stdio.h>
int main () {
  int meuNumero=15;
  char meuCaractere='D';
  float meuDecimal=1.3;
  
  printf("%d\n,%f\n",meuNumero,meuDecimal);
  printf("%f\n,%f\n",meuDecimal,meuDecimal);
  printf("%c\n,%f\n,%d\n",meuCaractere,meuDecimal,meuNumero);
  printf("%d\n,%c\n,%f\n,%d\n",meuNumero,meuCaractere,meuDecimal,meuNumero);
}
